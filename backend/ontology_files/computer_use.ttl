# Rule 11: Consecutive clicks without wait
# IF Step(?s1) AND hasAction(?s1, :click) AND nextStep(?s1, ?s2) AND hasAction(?s2, :click)
#    AND NOT EXISTS Step between with wait action
# THEN Warning("Consider adding wait between consecutive clicks")

