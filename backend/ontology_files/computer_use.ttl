# --- Cloud Services ---
:CloudService rdf:type owl:Class ;
    rdfs:subClassOf :WebService ;
    rdfs:label "Cloud Service"@en ;
    rdfs:comment "Cloud storage and services"@en .

# --- Developer Platforms ---
:DeveloperPlatform rdf:type owl:Class ;
    rdfs:subClassOf :WebService ;
    rdfs:label "Developer Platform"@en ;
    rdfs:comment "Software development platform"@en .

# --- Search Engines ---
:SearchEngine rdf:type owl:Class ;
    rdfs:subClassOf :WebService ;
    rdfs:label "Search Engine"@en ;
    rdfs:comment "Web search engine"@en .

# ============================================================
# EXECUTION STATE
# ============================================================

:ExecutionState rdf:type owl:Class ;
    rdfs:label "Execution State"@en ;
    rdfs:comment "State of step execution"@en .

:PendingState rdf:type owl:Class ;
    rdfs:subClassOf :ExecutionState ;
    rdfs:label "Pending"@en ;
    rdfs:comment "Step is waiting to be executed"@en .

:ExecutingState rdf:type owl:Class ;
    rdfs:subClassOf :ExecutionState ;
    rdfs:label "Executing"@en ;
    rdfs:comment "Step is currently being executed"@en .

:CompletedState rdf:type owl:Class ;
    rdfs:subClassOf :ExecutionState ;
    rdfs:label "Completed"@en ;
    rdfs:comment "Step has been successfully completed"@en .

:FailedState rdf:type owl:Class ;
    rdfs:subClassOf :ExecutionState ;
    rdfs:label "Failed"@en ;
    rdfs:comment "Step execution has failed"@en .

:SkippedState rdf:type owl:Class ;
    rdfs:subClassOf :ExecutionState ;
    rdfs:label "Skipped"@en ;
    rdfs:comment "Step was skipped"@en .

:RetryingState rdf:type owl:Class ;
    rdfs:subClassOf :ExecutionState ;
    rdfs:label "Retrying"@en ;
    rdfs:comment "Step is being retried after failure"@en .

# ============================================================
# OBJECT PROPERTIES
# ============================================================

:hasStep rdf:type owl:ObjectProperty ;
    rdfs:domain :Task ;
    rdfs:range :Step ;
    rdfs:label "has step"@en ;
    rdfs:comment "Task contains this step"@en .

:hasAction rdf:type owl:ObjectProperty ;
    rdfs:domain :Step ;
    rdfs:range :Action ;
    rdfs:label "has action"@en ;
    rdfs:comment "Step performs this action"@en .

:hasTarget rdf:type owl:ObjectProperty ;
    rdfs:domain :Step ;
    rdfs:range :UIElement ;
    rdfs:label "has target"@en ;
    rdfs:comment "Step targets this UI element"@en .

:requiresApplication rdf:type owl:ObjectProperty ;
    rdfs:domain :Task ;
    rdfs:range :Application ;
    rdfs:label "requires application"@en ;
    rdfs:comment "Task requires this application to be available"@en .

:opensApplication rdf:type owl:ObjectProperty ;
    rdfs:domain :OpenApplicationAction ;
    rdfs:range :Application ;
    rdfs:label "opens application"@en ;
    rdfs:comment "This action opens the specified application"@en .

:hasState rdf:type owl:ObjectProperty ;
    rdfs:domain :Step ;
    rdfs:range :ExecutionState ;
    rdfs:label "has state"@en ;
    rdfs:comment "Current execution state of the step"@en .

:nextStep rdf:type owl:ObjectProperty ;
    rdfs:domain :Step ;
    rdfs:range :Step ;
    rdfs:label "next step"@en ;
    rdfs:comment "The step that follows this one in sequence"@en .

:previousStep rdf:type owl:ObjectProperty ;
    owl:inverseOf :nextStep ;
    rdfs:domain :Step ;
    rdfs:range :Step ;
    rdfs:label "previous step"@en ;
    rdfs:comment "The step that precedes this one in sequence"@en .

:hasPrecondition rdf:type owl:ObjectProperty ;
    rdfs:domain :Step ;
    rdfs:range :ExecutionState ;
    rdfs:label "has precondition"@en ;
    rdfs:comment "Required state before step can execute"@en .

:hasPostcondition rdf:type owl:ObjectProperty ;
    rdfs:domain :Step ;
    rdfs:range :ExecutionState ;
    rdfs:label "has postcondition"@en ;
    rdfs:comment "Expected state after step execution"@en .

:partOf rdf:type owl:ObjectProperty ;
    rdfs:domain :UIElement ;
    rdfs:range :Application ;
    rdfs:label "part of"@en ;
    rdfs:comment "UI element belongs to this application"@en .

:usesService rdf:type owl:ObjectProperty ;
    rdfs:domain :Task ;
    rdfs:range :WebService ;
    rdfs:label "uses service"@en ;
    rdfs:comment "Task uses this web service"@en .

:compatibleWith rdf:type owl:ObjectProperty ;
    rdfs:domain :Application ;
    rdfs:range :Application ;
    rdfs:label "compatible with"@en ;
    rdfs:comment "Application is compatible with another"@en .

# ============================================================
# DATA PROPERTIES
# ============================================================

:stepOrder rdf:type owl:DatatypeProperty ;
    rdfs:domain :Step ;
    rdfs:range xsd:integer ;
    rdfs:label "step order"@en ;
    rdfs:comment "Sequential order of the step (1, 2, 3...)"@en .

:stepDescription rdf:type owl:DatatypeProperty ;
    rdfs:domain :Step ;
    rdfs:range xsd:string ;
    rdfs:label "step description"@en ;
    rdfs:comment "Human-readable description of the step"@en .

:expectedResult rdf:type owl:DatatypeProperty ;
    rdfs:domain :Step ;
    rdfs:range xsd:string ;
    rdfs:label "expected result"@en ;
    rdfs:comment "Expected outcome after step execution"@en .

:targetName rdf:type owl:DatatypeProperty ;
    rdfs:domain :Step ;
    rdfs:range xsd:string ;
    rdfs:label "target name"@en ;
    rdfs:comment "Name or label of the target UI element"@en .

:inputValue rdf:type owl:DatatypeProperty ;
    rdfs:domain :Step ;
    rdfs:range xsd:string ;
    rdfs:label "input value"@en ;
    rdfs:comment "Text or value to input (for type_text action)"@en .

:waitDuration rdf:type owl:DatatypeProperty ;
    rdfs:domain :Step ;
    rdfs:range xsd:integer ;
    rdfs:label "wait duration"@en ;
    rdfs:comment "Duration to wait in seconds"@en .

:isOptional rdf:type owl:DatatypeProperty ;
    rdfs:domain :Step ;
    rdfs:range xsd:boolean ;
    rdfs:label "is optional"@en ;
    rdfs:comment "Whether this step can be skipped"@en .

:taskGoal rdf:type owl:DatatypeProperty ;
    rdfs:domain :Task ;
    rdfs:range xsd:string ;
    rdfs:label "task goal"@en ;
    rdfs:comment "The goal or objective of the task"@en .

:originalInstruction rdf:type owl:DatatypeProperty ;
    rdfs:domain :Task ;
    rdfs:range xsd:string ;
    rdfs:label "original instruction"@en ;
    rdfs:comment "The original user instruction"@en .

:successCriteria rdf:type owl:DatatypeProperty ;
    rdfs:domain :Task ;
    rdfs:range xsd:string ;
    rdfs:label "success criteria"@en ;
    rdfs:comment "Criteria to determine if task succeeded"@en .

:applicationName rdf:type owl:DatatypeProperty ;
    rdfs:domain :Application ;
    rdfs:range xsd:string ;
    rdfs:label "application name"@en ;
    rdfs:comment "Name of the application"@en .

:keyName rdf:type owl:DatatypeProperty ;
    rdfs:domain :Step ;
    rdfs:range xsd:string ;
    rdfs:label "key name"@en ;
    rdfs:comment "Name of the key to press (for key_press action)"@en .

:url rdf:type owl:DatatypeProperty ;
    rdfs:domain :WebService ;
    rdfs:range xsd:anyURI ;
    rdfs:label "URL"@en ;
    rdfs:comment "Web address of the service"@en .

:executablePath rdf:type owl:DatatypeProperty ;
    rdfs:domain :Application ;
    rdfs:range xsd:string ;
    rdfs:label "executable path"@en ;
    rdfs:comment "Path to the application executable"@en .

:shortcutKey rdf:type owl:DatatypeProperty ;
    rdfs:domain :Action ;
    rdfs:range xsd:string ;
    rdfs:label "shortcut key"@en ;
    rdfs:comment "Keyboard shortcut for this action"@en .

:retryCount rdf:type owl:DatatypeProperty ;
    rdfs:domain :Step ;
    rdfs:range xsd:integer ;
    rdfs:label "retry count"@en ;
    rdfs:comment "Number of retry attempts allowed"@en .

:timeout rdf:type owl:DatatypeProperty ;
    rdfs:domain :Step ;
    rdfs:range xsd:integer ;
    rdfs:label "timeout"@en ;
    rdfs:comment "Maximum time to wait for step completion in seconds"@en .

# ============================================================
# INDIVIDUALS - Web Browsers
# ============================================================

:OperaBrowser rdf:type :Browser ;
    rdfs:label "Opera"@en ;
    rdfs:comment "Opera web browser"@en ;
    :applicationName "Opera" ;
    :executablePath "opera.exe" .

:ChromeBrowser rdf:type :Browser ;
    rdfs:label "Google Chrome"@en ;
    rdfs:comment "Google Chrome web browser"@en ;
