# --- Media Applications ---
:MediaApplication rdf:type owl:Class ;
    rdfs:subClassOf :Application ;
    rdfs:label "Media Application"@en ;
    rdfs:comment "Media playback and editing application"@en .

:MediaPlayer rdf:type owl:Class ;
    rdfs:subClassOf :MediaApplication ;
    rdfs:label "Media Player"@en ;
    rdfs:comment "Audio/video player application"@en .

:ImageEditor rdf:type owl:Class ;
    rdfs:subClassOf :MediaApplication ;
    rdfs:label "Image Editor"@en ;
    rdfs:comment "Image editing application"@en .

# --- Communication Applications ---
:CommunicationApp rdf:type owl:Class ;
    rdfs:subClassOf :Application ;
    rdfs:label "Communication App"@en ;
    rdfs:comment "Communication and messaging application"@en .

:EmailClient rdf:type owl:Class ;
    rdfs:subClassOf :CommunicationApp ;
    rdfs:label "Email Client"@en ;
    rdfs:comment "Email application"@en .

:Messenger rdf:type owl:Class ;
    rdfs:subClassOf :CommunicationApp ;
    rdfs:label "Messenger"@en ;
    rdfs:comment "Instant messaging application"@en .

:VideoConference rdf:type owl:Class ;
    rdfs:subClassOf :CommunicationApp ;
    rdfs:label "Video Conference"@en ;
    rdfs:comment "Video conferencing application"@en .

# --- System Utilities ---
:SystemUtility rdf:type owl:Class ;
    rdfs:subClassOf :Application ;
    rdfs:label "System Utility"@en ;
    rdfs:comment "System utility application"@en .

:FileManager rdf:type owl:Class ;
    rdfs:subClassOf :SystemUtility ;
    rdfs:label "File Manager"@en ;
    rdfs:comment "File management application"@en .

:CommandLine rdf:type owl:Class ;
    rdfs:subClassOf :SystemUtility ;
    rdfs:label "Command Line"@en ;
    rdfs:comment "Command line interface"@en .

# ============================================================
# WEB SERVICES - Online Platforms
# ============================================================

:WebService rdf:type owl:Class ;
    rdfs:label "Web Service"@en ;
    rdfs:comment "Online web service or platform"@en .

# --- Video Platforms ---
:VideoPlatform rdf:type owl:Class ;
    rdfs:subClassOf :WebService ;
    rdfs:label "Video Platform"@en ;
    rdfs:comment "Video streaming and sharing platform"@en .

# --- Social Media ---
:SocialMedia rdf:type owl:Class ;
    rdfs:subClassOf :WebService ;
    rdfs:label "Social Media"@en ;
