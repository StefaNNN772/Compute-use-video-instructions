# THEN Warning("Consider opening application first")

# Rule 2: wait should follow open_application
# IF Step(?s1) AND hasAction(?s1, :open_application) AND nextStep(?s1, ?s2)
#    AND NOT hasAction(?s2, :wait)
# THEN Warning("Recommended to add wait after open_application")

# Rule 3: type_text requires non-empty value
# IF Step(?s) AND hasAction(?s, :type_text) AND (NOT inputValue(?s, ?val) OR ?val = "")
# THEN Error("type_text action requires a value to type")

# Rule 4: key_press requires key name
# IF Step(?s) AND hasAction(?s, :key_press) AND NOT keyName(?s, ?key)
#    AND NOT targetName(?s, ?target)
# THEN Error("key_press action requires a key name")

# Rule 5: click requires target
# IF Step(?s) AND hasAction(?s, :click) AND (NOT targetName(?s, ?t) OR ?t = "screen")
# THEN Warning("click action should have a specific target element")

# Rule 6: wait requires duration
# IF Step(?s) AND hasAction(?s, :wait) AND NOT waitDuration(?s, ?d)
# THEN Error("wait action requires duration in seconds")

# Rule 7: Browser actions require browser to be open
# IF Step(?s) AND targetName(?s, "Address bar") 
