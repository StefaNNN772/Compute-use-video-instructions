@prefix : <http://example.org/computer-use#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

# ============================================================
# ONTOLOGY DECLARATION
# ============================================================

<http://example.org/computer-use> rdf:type owl:Ontology ;
    rdfs:label "Computer Use Ontology"@en ;
    rdfs:comment "Ontology for desktop application automation and video tutorial creation"@en ;
    dc:creator "Computer Use Project" ;
    dc:description "This ontology defines concepts for automating desktop applications including browsers, IDEs, text editors, and various web services."@en ;
    owl:versionInfo "2.0" .

# ============================================================
# CLASSES - Core Classes
# ============================================================

# === TASK (Composite Process) ===
:Task rdf:type owl:Class ;
    rdfs:label "Task"@en ;
    rdfs:comment "A complete task consisting of multiple steps to achieve a goal"@en .

# === STEP (Atomic Process) ===
:Step rdf:type owl:Class ;
    rdfs:label "Step"@en ;
    rdfs:comment "A single atomic step in task execution"@en .

# ============================================================
# ACTION TYPES - All Available Actions
# ============================================================

:Action rdf:type owl:Class ;
    rdfs:label "Action"@en ;
    rdfs:comment "Base class for all executable actions"@en .

# --- Mouse Actions ---
:MouseAction rdf:type owl:Class ;
    rdfs:subClassOf :Action ;
    rdfs:label "Mouse Action"@en ;
    rdfs:comment "Actions performed with the mouse"@en .

:ClickAction rdf:type owl:Class ;
    rdfs:subClassOf :MouseAction ;
    rdfs:label "Click Action"@en ;
    rdfs:comment "Single left mouse button click"@en .

:DoubleClickAction rdf:type owl:Class ;
    rdfs:subClassOf :MouseAction ;
    rdfs:label "Double Click Action"@en ;
    rdfs:comment "Double left mouse button click"@en .

:RightClickAction rdf:type owl:Class ;
    rdfs:subClassOf :MouseAction ;
    rdfs:label "Right Click Action"@en ;
    rdfs:comment "Right mouse button click for context menu"@en .

:MoveMouseAction rdf:type owl:Class ;
    rdfs:subClassOf :MouseAction ;
    rdfs:label "Move Mouse Action"@en ;
    rdfs:comment "Move mouse cursor to specific coordinates"@en .

:DragAndDropAction rdf:type owl:Class ;
    rdfs:subClassOf :MouseAction ;
    rdfs:label "Drag and Drop Action"@en ;
    rdfs:comment "Drag element from one location to another"@en .

:ScrollAction rdf:type owl:Class ;
    rdfs:subClassOf :MouseAction ;
    rdfs:label "Scroll Action"@en ;
    rdfs:comment "Scroll up or down on the page"@en .

# --- Keyboard Actions ---
:KeyboardAction rdf:type owl:Class ;
    rdfs:subClassOf :Action ;
    rdfs:label "Keyboard Action"@en ;
    rdfs:comment "Actions performed with the keyboard"@en .

:TypeTextAction rdf:type owl:Class ;
    rdfs:subClassOf :KeyboardAction ;
    rdfs:label "Type Text Action"@en ;
    rdfs:comment "Type text into an input field or editor"@en .

:KeyPressAction rdf:type owl:Class ;
    rdfs:subClassOf :KeyboardAction ;
    rdfs:label "Key Press Action"@en ;
    rdfs:comment "Press a single key (Enter, Escape, F5, etc.)"@en .

:KeyCombinationAction rdf:type owl:Class ;
    rdfs:subClassOf :KeyboardAction ;
    rdfs:label "Key Combination Action"@en ;
    rdfs:comment "Press multiple keys simultaneously (Ctrl+C, Alt+F4, Ctrl+Shift+S)"@en .

# --- Application Actions ---
:ApplicationAction rdf:type owl:Class ;
    rdfs:subClassOf :Action ;
    rdfs:label "Application Action"@en ;
    rdfs:comment "Actions related to application lifecycle"@en .

:OpenApplicationAction rdf:type owl:Class ;
    rdfs:subClassOf :ApplicationAction ;
    rdfs:label "Open Application Action"@en ;
    rdfs:comment "Launch an application"@en .

:CloseApplicationAction rdf:type owl:Class ;
    rdfs:subClassOf :ApplicationAction ;
    rdfs:label "Close Application Action"@en ;
    rdfs:comment "Close an application"@en .

:SwitchApplicationAction rdf:type owl:Class ;
    rdfs:subClassOf :ApplicationAction ;
    rdfs:label "Switch Application Action"@en ;
    rdfs:comment "Switch to another running application (Alt+Tab)"@en .

:MinimizeApplicationAction rdf:type owl:Class ;
    rdfs:subClassOf :ApplicationAction ;
    rdfs:label "Minimize Application Action"@en ;
    rdfs:comment "Minimize application window"@en .

:MaximizeApplicationAction rdf:type owl:Class ;
    rdfs:subClassOf :ApplicationAction ;
    rdfs:label "Maximize Application Action"@en ;
    rdfs:comment "Maximize application window"@en .

# --- System Actions ---
:SystemAction rdf:type owl:Class ;
    rdfs:subClassOf :Action ;
    rdfs:label "System Action"@en ;
    rdfs:comment "System-level actions"@en .

:WaitAction rdf:type owl:Class ;
    rdfs:subClassOf :SystemAction ;
    rdfs:label "Wait Action"@en ;
    rdfs:comment "Wait for a specified duration in seconds"@en .

:ScreenshotAction rdf:type owl:Class ;
    rdfs:subClassOf :SystemAction ;
    rdfs:label "Screenshot Action"@en ;
    rdfs:comment "Take a screenshot of the current screen"@en .

:CopyAction rdf:type owl:Class ;
    rdfs:subClassOf :SystemAction ;
    rdfs:label "Copy Action"@en ;
    rdfs:comment "Copy selected content to clipboard"@en .

:PasteAction rdf:type owl:Class ;
    rdfs:subClassOf :SystemAction ;
    rdfs:label "Paste Action"@en ;
    rdfs:comment "Paste content from clipboard"@en .

# ============================================================
# UI ELEMENTS - User Interface Components
# ============================================================

:UIElement rdf:type owl:Class ;
    rdfs:label "UI Element"@en ;
    rdfs:comment "Base class for all user interface elements"@en .

# --- Clickable Elements ---
:ClickableElement rdf:type owl:Class ;
    rdfs:subClassOf :UIElement ;
    rdfs:label "Clickable Element"@en ;
    rdfs:comment "Elements that can be clicked"@en .

:Button rdf:type owl:Class ;
    rdfs:subClassOf :ClickableElement ;
    rdfs:label "Button"@en ;
    rdfs:comment "A clickable button element"@en .

:Link rdf:type owl:Class ;
    rdfs:subClassOf :ClickableElement ;
    rdfs:label "Link"@en ;
    rdfs:comment "A hyperlink element"@en .

:Icon rdf:type owl:Class ;
    rdfs:subClassOf :ClickableElement ;
    rdfs:label "Icon"@en ;
    rdfs:comment "A clickable icon"@en .

:Tab rdf:type owl:Class ;
    rdfs:subClassOf :ClickableElement ;
    rdfs:label "Tab"@en ;
    rdfs:comment "A tab element in tabbed interface"@en .

:MenuItem rdf:type owl:Class ;
    rdfs:subClassOf :ClickableElement ;
    rdfs:label "Menu Item"@en ;
    rdfs:comment "An item in a menu or dropdown"@en .

:Checkbox rdf:type owl:Class ;
    rdfs:subClassOf :ClickableElement ;
    rdfs:label "Checkbox"@en ;
    rdfs:comment "A checkbox element"@en .

:RadioButton rdf:type owl:Class ;
    rdfs:subClassOf :ClickableElement ;
    rdfs:label "Radio Button"@en ;
    rdfs:comment "A radio button element"@en .

# --- Input Elements ---
:InputElement rdf:type owl:Class ;
    rdfs:subClassOf :UIElement ;
    rdfs:label "Input Element"@en ;
    rdfs:comment "Elements that accept user input"@en .

:TextField rdf:type owl:Class ;
    rdfs:subClassOf :InputElement ;
    rdfs:label "Text Field"@en ;
    rdfs:comment "A single-line text input field"@en .

:TextArea rdf:type owl:Class ;
    rdfs:subClassOf :InputElement ;
    rdfs:label "Text Area"@en ;
    rdfs:comment "A multi-line text input area"@en .

:SearchBox rdf:type owl:Class ;
    rdfs:subClassOf :TextField ;
    rdfs:label "Search Box"@en ;
    rdfs:comment "A search input field"@en .

:AddressBar rdf:type owl:Class ;
    rdfs:subClassOf :TextField ;
    rdfs:label "Address Bar"@en ;
    rdfs:comment "Browser URL address bar"@en .

:CodeEditor rdf:type owl:Class ;
    rdfs:subClassOf :TextArea ;
    rdfs:label "Code Editor"@en ;
    rdfs:comment "Code editing area in IDE"@en .

:Terminal rdf:type owl:Class ;
    rdfs:subClassOf :TextArea ;
    rdfs:label "Terminal"@en ;
    rdfs:comment "Command line terminal"@en .

:Dropdown rdf:type owl:Class ;
    rdfs:subClassOf :InputElement ;
    rdfs:label "Dropdown"@en ;
    rdfs:comment "A dropdown select element"@en .

# --- Container Elements ---
:ContainerElement rdf:type owl:Class ;
    rdfs:subClassOf :UIElement ;
    rdfs:label "Container Element"@en ;
    rdfs:comment "Elements that contain other elements"@en .

:Window rdf:type owl:Class ;
    rdfs:subClassOf :ContainerElement ;
    rdfs:label "Window"@en ;
    rdfs:comment "An application window"@en .

:Dialog rdf:type owl:Class ;
    rdfs:subClassOf :ContainerElement ;
    rdfs:label "Dialog"@en ;
    rdfs:comment "A dialog or modal window"@en .

:Panel rdf:type owl:Class ;
    rdfs:subClassOf :ContainerElement ;
    rdfs:label "Panel"@en ;
    rdfs:comment "A panel or pane in the interface"@en .

:Menu rdf:type owl:Class ;
    rdfs:subClassOf :ContainerElement ;
    rdfs:label "Menu"@en ;
    rdfs:comment "A menu container"@en .

:Toolbar rdf:type owl:Class ;
    rdfs:subClassOf :ContainerElement ;
    rdfs:label "Toolbar"@en ;
    rdfs:comment "A toolbar with action buttons"@en .

:Sidebar rdf:type owl:Class ;
    rdfs:subClassOf :ContainerElement ;
    rdfs:label "Sidebar"@en ;
    rdfs:comment "A sidebar panel"@en .

# --- Display Elements ---
:DisplayElement rdf:type owl:Class ;
    rdfs:subClassOf :UIElement ;
    rdfs:label "Display Element"@en ;
    rdfs:comment "Elements that display information"@en .

:Label rdf:type owl:Class ;
    rdfs:subClassOf :DisplayElement ;
    rdfs:label "Label"@en ;
    rdfs:comment "A text label"@en .

:Image rdf:type owl:Class ;
    rdfs:subClassOf :DisplayElement ;
    rdfs:label "Image"@en ;
    rdfs:comment "An image element"@en .

:VideoPlayer rdf:type owl:Class ;
    rdfs:subClassOf :DisplayElement ;
    rdfs:label "Video Player"@en ;
    rdfs:comment "A video player component"@en .

:ProgressBar rdf:type owl:Class ;
    rdfs:subClassOf :DisplayElement ;
    rdfs:label "Progress Bar"@en ;
    rdfs:comment "A progress indicator"@en .

:Notification rdf:type owl:Class ;
    rdfs:subClassOf :DisplayElement ;
    rdfs:label "Notification"@en ;
    rdfs:comment "A notification or toast message"@en .

# ============================================================
# APPLICATIONS - Application Categories
# ============================================================

:Application rdf:type owl:Class ;
    rdfs:label "Application"@en ;
    rdfs:comment "Base class for all desktop and web applications"@en .

# --- Web Browsers ---
:Browser rdf:type owl:Class ;
    rdfs:subClassOf :Application ;
    rdfs:label "Web Browser"@en ;
    rdfs:comment "Web browser application"@en .

# --- IDEs and Code Editors ---
:IDE rdf:type owl:Class ;
    rdfs:subClassOf :Application ;
    rdfs:label "IDE"@en ;
    rdfs:comment "Integrated Development Environment"@en .

:CodeEditor rdf:type owl:Class ;
    rdfs:subClassOf :Application ;
    rdfs:label "Code Editor Application"@en ;
    rdfs:comment "Lightweight code editor application"@en .

# --- Text Editors ---
:TextEditor rdf:type owl:Class ;
    rdfs:subClassOf :Application ;
    rdfs:label "Text Editor"@en ;
    rdfs:comment "Plain text editor application"@en .

# --- Office Applications ---
:OfficeApplication rdf:type owl:Class ;
    rdfs:subClassOf :Application ;
    rdfs:label "Office Application"@en ;
    rdfs:comment "Office productivity application"@en .

:WordProcessor rdf:type owl:Class ;
    rdfs:subClassOf :OfficeApplication ;
    rdfs:label "Word Processor"@en ;
    rdfs:comment "Document editing application"@en .

:Spreadsheet rdf:type owl:Class ;
    rdfs:subClassOf :OfficeApplication ;
    rdfs:label "Spreadsheet"@en ;
    rdfs:comment "Spreadsheet application"@en .

:Presentation rdf:type owl:Class ;
    rdfs:subClassOf :OfficeApplication ;
    rdfs:label "Presentation"@en ;
    rdfs:comment "Presentation software"@en .

# --- Developer Tools ---
:DeveloperTool rdf:type owl:Class ;
    rdfs:subClassOf :Application ;
    rdfs:label "Developer Tool"@en ;
    rdfs:comment "Software development tools"@en .

:ContainerPlatform rdf:type owl:Class ;
    rdfs:subClassOf :DeveloperTool ;
