# THEN Warning("Ensure YouTube is loaded before interacting with its elements")

# Rule 9: IDE project actions require project to be created
# IF Step(?s) AND hasAction(?s, :type_text) AND targetName(?s, "editor")
#    AND NOT EXISTS previousStep creating/opening project
# THEN Warning("Ensure project is created before writing code")

# Rule 10: Save before run
# IF Step(?s1) AND hasAction(?s1, :key_press) AND keyName(?s1, "f5")
